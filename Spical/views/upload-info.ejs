					<li><a href="sass.html"><i class="material-icons fas fa-chevron-left"></i></a></li>
				</ul>
				<ul id="nav-mobile2" class="right">
					<li><a href="sass.html"><i class="material-icons sidenav-trigger"
								data-target="side-menu">menu</i></a></li>
				</ul>
			</div>
		</nav>
	</div>
	<!-- side nav -->
	<ul id="side-menu" class="sidenav side-menu">
