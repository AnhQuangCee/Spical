	<div class="icon-logo">
		<div class="container">
			<div class="row">
				<div class="col s12 center">
					<img src="images/upload2.png" alt="">
				</div>
			</div>
		</div>
	</div>
	<!-- end icon-logo -->

	<div class="create-upload">
		<div class="container">
			<div class="row">
				<div class="col s12">
					<h5 class="center">Upload file</h5>
					<p class="center">Browse and choose files
						you want to upload</p>
				</div>
			</div>
